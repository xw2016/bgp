@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_833416_y8tgmmbbq0j.eot?t=1536830104261'); /* IE9*/
  src: url('//at.alicdn.com/t/font_833416_y8tgmmbbq0j.eot?t=1536830104261#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('//at.alicdn.com/t/font_833416_y8tgmmbbq0j.ttf?t=1536830104261') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('//at.alicdn.com/t/font_833416_y8tgmmbbq0j.svg?t=1536830104261#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-zuo:before {
  content: "\e656";
}

.icon-gou:before {
  content: "\e632";
}

.icon-sousuo:before {
  content: "\e605";
}

.icon-cha:before {
  content: "\e617";
}

/* pages/ContactList/ContactList.wxss */

page {
  background: #efefef;
}

.searchLetter {
  position: fixed;
  right: 0px;
  top: 45px;
  width: 30px;
  height: calc(100vh - 45px);
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  color: #333;
  z-index: 50;
}

.searchLetter.active {
  background: #efefef;
  opacity: 0.6;
}

.searchLetter.active .jumpLetter {
  color: #333;
}

.jumpLetter {
  width: 30px;
  color: #333;
  font-size: 12px;
  line-height: 17px !important;
  height: 17px;
  text-align: center;
}

.touchClass {
  background-color: transparent;
  color: #fff;
}

.showSlectedLetter {
  background-color: rgba(0, 0, 0, 0.5);
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 50%;
  left: 50%;
  margin: -100rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 20rpx;
  font-size: 52rpx;
  z-index: 1;
}

.selection {
  display: flex;
  width: 100%;
  flex-direction: column;
}

.item_letter {
  display: flex;
  background-color: #f5f5f5;
  height: 20px;
  line-height: 20px;
  padding-left: 12px;
  align-items: center;
  font-size: 12px;
  color: #666;
}

.item_city {
  display: flex;
  background-color: #fff;
  width: 100%;
  box-sizing: border-box;
  height: 40px;
  padding-left: 34rpx;
  align-items: center;
  border-bottom: 1rpx solid #ededed;
  font-size: 12px;
  color: #666;
  position: relative;
}

.item_city .checkbox {
  position: absolute;
  top: 11px;
  right: 30px;
  width: 18px;
  height: 18px;
  border: 1px solid #cdcdcd;
  border-radius: 3px;
  overflow: hidden;
}

.item_city .checkbox .iconfont {
  display: none;
  color: #fff;
  background: #15cd15;
  line-height: 18px;
  width: 18px;
  text-align: center;
  padding: 0px;
  margin: 0px;
  font-size: 8px;
}

.item_city .checkbox.active .iconfont {
  display: block;
}

.item_city .checkbox.active {
  background: #15cd15;
  border: none;
  border-radius: 3px;
}

.item_city i {
  background: #e64949;
  color: #fff;
  padding: 2px 5px;
  border-radius: 8rpx;
  margin-left: 5px;
  font-size: 14px;
}

.searchCtr {
  position: fixed;
  top: 2px;
  left: 0px;
  box-sizing: border-box;
  width: 100%;
  height: 40px;
  z-index: 50;
  background: #fff;
  padding: 0px 10px 3px 10px;
  box-shadow: 1px 1px 1px 1px #dedede;
}

.searchBar {
  width: 100%;
  height: 37px;
  border-bottom: 1px solid #cdcdcd;
  color: #cdcdcd;
  overflow: hidden;
  white-space: nowrap;
}

.searchBar.active {
  border-bottom: 1px solid #15cd15;
}

.searchBar .iconfont, .searchBar .searchInput {
  display: block;
  float: left;
}

.searchBar .iconfont {
  width: 20px;
  height: 37px;
  line-height: 37px;
  color: #999;
  font-weight: bold;
  font-size: 18px;
  text-align: right;
}

.searchBar .searchInput {
  display: flex;
  min-width: calc(50vw - 50px);
  line-height: 37px;
  height: 37px;
  color: #333;
  padding-left: 10px;
}

.searchBar .searchInputPH {
  color: #999;
}

.searchBar .cancel {
  position: absolute;
  right: 10px;
  top: 6.5px;
  width: 24px;
  height: 24px;
  text-align: center;
  border-radius: 12.5px;
  background: #cdcdcd;
  padding: 0px;
  margin: 0px;
}

.searchBar .cancel .iconfont {
  width: 24px;
  height: 24px;
  line-height: 24px;
  font-size: 12px;
  color: #fff;
  text-align: center;
  display: block;
}

.labelTitle {
  position: fixed;
  left: 0px;
  top: -45px;
  width: 100vw;
  height: 45px;
  line-height: 45px;
  font-size: 16px;
  color: #333;
  background: #fff;
  float: left;
  display: block;
}

.labelTitle .cancel::after {
  border: none;
}

.labelTitle .cancel {
  display: block;
  float: left;
  width: 30px;
  height: 45px;
  text-align: center;
  background: #fff;
  padding: 0px;
  margin: 0px;
  border: none;
}

.labelTitle .cancel .iconfont {
  width: 30px;
  height: 45px;
  line-height: 45px;
  font-size: 16px;
  color: #333;
  text-align: center;
  display: block;
}

.labelView {
  box-sizing: border-box;
  position: fixed;
  top: 45px;
  left: 0px;
  width: 100vw;
  height: calc(100vh - 92px);
  padding: 5px;
  background: #fff;
  z-index: 80;
}

.labelHint {
  box-sizing: border-box;
  width: 100%;
  padding: 5px 0px 8px 0px;
  font-size: 12px;
}

.label {
  display: inline-block;
  box-sizing: border-box;
  line-height: 20px;
  padding: 5px 8px;
  border-radius: 20px;
  border: 1px solid #cdcdcd;
  background: #fff;
  margin: 0px 5px 5px 0px;
  height: 30px;
  white-space: nowrap;
  font-size: 12px;
}

.userListScrollView {
  width: 100vw;
  height: calc(100vh - 92px);
}

.btnCtr {
  position: fixed;
  bottom: 0px;
  left: 0px;
  box-sizing: border-box;
  width: 100vw;
  height: 45px;
  padding: 5px;
  z-index: 50;
  background: #333;
}

.btnCtr .theSelectedUsers {
  display: block;
  float: left;
  overflow-x: scroll;
  max-width: 200px;
  height: 35px;
  white-space: nowrap;
}

.btnCtr .theSelectedUsers .selectedUser {
  box-sizing: border-box;
  display: inline-block;
  width: 42px;
  height: 35px;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 37px;
  color: #333;
  border: none;
  font-size: 10px;
  background: #fefefe;
  border-radius: 2px;
  text-align: center;
  margin-right: 8px;
  padding: 0px 2px;
}

.btnCtr .confirmBtn[disabled] {
  border: none;
  color: #13ac13;
  background: transparent;
}

.btnCtr .confirmBtn {
  display: block;
  float: right;
  background: transparent;
  width: 60px;
  height: 35px;
  line-height: 35px;
  text-align: center;
  padding: 0px;
  margin: 0px;
  border: none;
  font-size: 14px;
  color: #15cd15;
}

.btnCtr .confirmBtn::after {
  border: none;
}

.searchResultCtr {
  position: fixed;
  left: 0px;
  top: 45px;
  width: 100vw;
  height: calc(100vh - 92px);
  padding: 0px;
  margin: 0px;
  z-index: 120;
  background: #fff;
}

.searchNoData {
  margin-top: 20px;
  width: 100vw;
  line-height: 25px;
  font-size: 14px;
  color: #ababab;
  text-align: center;
  overflow: hidden;
}

.searchNoData view:nth-child(1), .searchNoData view:nth-child(3) {
  color: #666;
}

.searchNoData view {
  display: inline-block;
  color: #15cd15;
  max-width: calc(100vw - 100px);
  font-size: 14px;
  overflow: hidden;
  word-break: break-all;
}
